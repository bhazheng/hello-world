import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt(); int b = in.nextInt();
        for(int i = 1; i<=a; i++){
            if(i%b!=0){
                System.out.print(i+" ");
            }else{
                System.out.print("*"+" ");
            }
        }
    }
}
