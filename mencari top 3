import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int[] b = new int[a];
        for (int j = 0; j < b.length; j++) {
            b[j] = in.nextInt();
        }
        Arrays.sort(b);
        for (int j = a-1; j > a-4; j--) {
            System.out.print(b[j]+" ");
        }
    }
}
